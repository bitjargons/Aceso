	<header class="intro-header" style="background-image: url(https://images.unsplash.com/photo-1492057201103-01c5991d4596?ixlib=rb-0.3.5&s=9b5e92f58ec920c4946edc083e2cd0df&auto=format&fit=crop&w=755&q=80);>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
          <div class="site-heading">
            <h1>Aceso</h1>
            <hr class="small">
            <span class="subheading">An Initiative by Jargons</span>
            <button type="button" class="btn btn-primary btn-lg" data-toggle="modal" data-target="#myModal">
            TAKE A TEST :)</button>
          </div>
        </div>
      </div>
    </div>
  </header>
 	<div class="container">


<!-- The Modal -->
<div class="modal fade" id="myModal">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">

      <!-- Modal Header -->
      <div class="modal-header">
        <h2 class="modal-title">Questions</h2>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
      </div>

      <!-- Modal body -->
      <div class="modal-body">
          <div class="questionsBox">
        <div id='quiz'></div>
         <div class="questionsRow">
        <div class='button' id='next'><a href='#'>Next</a></div>
        <div class='button' id='prev'><a href='#'>Prev</a></div>
        <div class='button' id='start'> <a href='#'>Start Over</a></div>
      </div>
      <div class="modal-footer">
        <div class="progress progress-striped active">
          <div class="progress-bar"  role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 0%" id ="progress">
            <span class="sr-only">5% Complete</span>
          </div>
        </div>
        <form action="/users/<%= currentUser._id %>/test" method="post">
          <input type="text" name="name" id="testbox" value="DASS" hidden="true">
          <input type="text" name="score[stress]" id="stressbox" hidden="true">
          <input type="text" name="score[anxiety]" id="anxietybox" hidden="true">
          <input type="text" name="score[depression]" id="depressionbox" hidden="true">
          <input type="text" name="isCritical" id="criticalbox" hidden="true">
          <button type="submit" id="postit" hidden="true">Send Results</button>
        </form>
      </div>
        </div>
     </div>
    </div>
  </div>
</div>

<script type='text/javascript' src='https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js'></script>
<script type="text/javascript" src='questions.json'></script>
<script type='text/javascript' src='/scripts/tr1.js'></script>
 	
